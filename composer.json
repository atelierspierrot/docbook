{
    "name": "atelierspierrot/docbook",
    "title": "DocBook",
    "type": "library",
    "description": "Simple CMS PHP app to build rich HTML5 views from a Markdown contents filesystem.",
    "keywords": [ "cms", "markdown" ],
    "homepage": "http://github.com/atelierspierrot/docbook",
    "version": "1.3.4",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Les Ateliers Pierrot & contributors",
            "email": "contact@ateliers-pierrot.fr",
            "homepage": "http://www.ateliers-pierrot.fr/"
        },{
            "name": "Pierre Cassat",
            "email": "piwi@ateliers-pierrot.fr",
            "homepage": "http://github.com/pierowbmstr"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "lib-curl": "*",
        "lib-iconv": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-posix": "*",
        "ext-SPL": "*",
        "ext-xml": "*",
        "psr/log": "dev-master",
        "atelierspierrot/patterns": "*",
        "atelierspierrot/library": "*",
        "atelierspierrot/webfilesystem": "dev-dev",
        "picas/markdown-extended": "v0.1.0-beta",
        "atelierspierrot/internationalization": "dev-master",
        "twig/twig": "1.*"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "sami/sami": "dev-master",
        "dropbox/dropbox-sdk": "1.1.*"
    },
    "autoload": { "psr-0": { 
        "DocBook": "src",
        "DocBook_": "src"
    } },
    "config": {
        "vendor-dir": "src/vendor"
    },
    "scripts": {
        "post-update-cmd": "DocBook\\Composer\\Scripts::emptyCache",
        "post-install-cmd": "DocBook\\Composer\\Scripts::emptyCache"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.*-dev"
        }
    }
}
